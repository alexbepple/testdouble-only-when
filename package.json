{
  "version": "1.1.0",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "run-s clean",
    "build": "rollup --config",
    "test": "mocha --opts mocha.opts",
    "prepublishOnly": "run-p --aggregate-output --print-name test build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepublishOnly && lint-staged"
    }
  },
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "hamjest": "^3.1.0",
    "husky": "^1.0.0-rc.1",
    "lint-staged": "^7.0.4",
    "mocha": "^5.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "1.12.1",
    "reify": "^0.13.7",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "standard-version": "^4.4.0",
    "testdouble": "^3.6.0"
  },
  "peerDependencies": {
    "testdouble": ">= 1"
  },
  "name": "testdouble-only-when",
